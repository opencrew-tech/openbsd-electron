{
  "name": "@tailwindcss/oxide",
  "version": "4.1.16",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tailwindlabs/tailwindcss.git",
    "directory": "crates/node"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "binaryName": "tailwindcss-oxide",
    "packageName": "@tailwindcss/oxide",
    "targets": [
      "armv7-linux-androideabi",
      "aarch64-linux-android",
      "aarch64-apple-darwin",
      "aarch64-unknown-linux-gnu",
      "aarch64-unknown-linux-musl",
      "armv7-unknown-linux-gnueabihf",
      "x86_64-unknown-linux-musl",
      "x86_64-unknown-freebsd",
      "i686-pc-windows-msvc",
      "aarch64-pc-windows-msvc",
      "wasm32-wasip1-threads"
    ],
    "wasm": {
      "initialMemory": 16384,
      "browser": {
        "fs": true
      }
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^3.3.0",
    "@napi-rs/wasm-runtime": "^1.0.7",
    "emnapi": "1.5.0"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run build:platform && pnpm run build:wasm",
    "build:platform": "napi build --platform --release",
    "postbuild:platform": "node ./scripts/move-artifacts.mjs",
    "build:wasm": "napi build --release --target wasm32-wasip1-threads",
    "postbuild:wasm": "node ./scripts/move-artifacts.mjs",
    "dev": "cargo watch --quiet --shell 'npm run build'",
    "build:debug": "napi build --platform",
    "version": "napi version"
  },
  "pnpm": {
    "overrides": {
      "lightningcss": "npm:lightningcss-wasm",
      "rollup": "npm:@rollup/wasm-node",
      "@parcel/watcher": "link:../../../node/parcel-watcher/node_modules/@parcel/watcher",
      "esbuild": "link:../../../node/esbuild/node_modules/esbuild"
    }
  }
}
